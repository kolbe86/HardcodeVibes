<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="de.nak.librarymgmt.model.LendingProcess" table="LENDINGPROCESS">
		<id name="lendingProcessID" column="LENDING_PROCESS_ID" type="long">
			<generator class="native" />
		</id>
		<property name="issueDate" column="ISSUE_DATE" type="date" />
		<property name="returnDate" column="RETURN_DATE" type="date" />
		<property name="extensionOfTimeLevel" column="EXTENSION_OF_TIME_LEVEL"
			not-null="true" type="integer" />
<!--		<many-to-one name = "borrower " not-null="true" column="BORROWER_ID" /> -->
	</class>
</hibernate-mapping>
