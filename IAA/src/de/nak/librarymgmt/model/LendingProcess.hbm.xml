<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="de.nak.librarymgmt.model.LendingProcess" table="LENDINGPROCESS">
		<id name="lendingProcessID" column="LENDINGPROCESS_ID" type="long">
			<generator class="sequence" />
		</id>
		<many-to-one name="borrower" column="borrowerID" unique="true" not-null="true"></many-to-one>
<!-- 		<many-to-one name="publication" column="PUBLICATION_ID" unique="true" not-null="true"></many-to-one> -->
		<property name="issueDate" column="ISSUE_DATE"
			type="timestamp" />
		<property name="returnDate" column="RETURN_DATE"
			type="timestamp" />
		<property name="extensionOfTimeLevel" column="EXTENSION_OF_TIME_LEVEL" not-null="true"></property>
		<property name="state" column="STATE" not-null="true"></property>
	</class>
</hibernate-mapping>